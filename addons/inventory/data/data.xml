<?xml version="1.0" encoding="utf-8"?>
<ocean>
    <data noupdate="1">
        
        <!-- Stock Locations -->
        <record id="stock_location_company" model="stock.location">
            <field name="name">Company</field>
            <field name="code">COMPANY</field>
            <field name="description">Company root location</field>
            <field name="location_type">view</field>
            <field name="usage">view</field>
            <field name="is_active">True</field>
            <field name="company_id" ref="base.main_company"/>
        </record>
        
        <record id="stock_location_warehouse" model="stock.location">
            <field name="name">Main Warehouse</field>
            <field name="code">WH</field>
            <field name="description">Main warehouse location</field>
            <field name="location_type">internal</field>
            <field name="usage">internal</field>
            <field name="is_active">True</field>
            <field name="parent_id" ref="stock_location_company"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="max_capacity">10000.0</field>
            <field name="temperature_controlled">True</field>
            <field name="min_temperature">18.0</field>
            <field name="max_temperature">25.0</field>
            <field name="age_group_restriction">none</field>
        </record>
        
