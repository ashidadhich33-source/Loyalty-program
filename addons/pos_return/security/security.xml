        <!-- POS Return Manager Group -->
        <record id="group_pos_return_manager" model="ocean.groups">
            <field name="name">POS Return Manager</field>
            <field name="category_id" ref="base.module_category_sales"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
        
        <!-- POS Return User Group -->
        <record id="group_pos_return_user" model="ocean.groups">
            <field name="name">POS Return User</field>
            <field name="category_id" ref="base.module_category_sales"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
        
    </data>
</ocean>