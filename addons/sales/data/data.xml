<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sales Order Sequence -->
        <record id="seq_sale_order" model="ir.sequence">
            <field name="name">Sales Order</field>
            <field name="code">sale.order</field>
            <field name="prefix">SO</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Sales Quotation Sequence -->
        <record id="seq_sale_quotation" model="ir.sequence">
            <field name="name">Sales Quotation</field>
            <field name="code">sale.quotation</field>
            <field name="prefix">SQ</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Sales Delivery Sequence -->
        <record id="seq_sale_delivery" model="ir.sequence">
            <field name="name">Sales Delivery</field>
            <field name="code">sale.delivery</field>
            <field name="prefix">SD</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Sales Return Sequence -->
        <record id="seq_sale_return" model="ir.sequence">
            <field name="name">Sales Return</field>
            <field name="code">sale.return</field>
            <field name="prefix">SR</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Default Sales Team -->
        <record id="default_sales_team" model="sale.team">
            <field name="name">Default Sales Team</field>
            <field name="code">DEFAULT</field>
            <field name="description">Default sales team for kids clothing</field>
            <field name="age_group_focus">all</field>
            <field name="gender_focus">all</field>
            <field name="season_focus">all_season</field>
        </record>

        <!-- Default Sales Territory -->
        <record id="default_sales_territory" model="sale.territory">
            <field name="name">Default Territory</field>
            <field name="code">DEFAULT</field>
            <field name="description">Default territory for kids clothing</field>
            <field name="age_group_focus">all</field>
            <field name="gender_focus">all</field>
            <field name="season_focus">all_season</field>
        </record>

        <!-- Default Sales Commission -->
        <record id="default_sales_commission" model="sale.commission">
            <field name="name">Default Commission</field>
            <field name="code">DEFAULT</field>
            <field name="description">Default commission for kids clothing</field>
            <field name="commission_type">percentage</field>
            <field name="commission_rate">5.0</field>
            <field name="age_group_focus">all</field>
            <field name="gender_focus">all</field>
            <field name="season_focus">all_season</field>
            <field name="state">active</field>
        </record>

        <!-- Kids Clothing Specific Sales Teams -->
        <record id="baby_sales_team" model="sale.team">
            <field name="name">Baby Sales Team</field>
            <field name="code">BABY</field>
            <field name="description">Sales team for baby clothing (0-2 years)</field>
            <field name="age_group_focus">0-2</field>
            <field name="gender_focus">all</field>
            <field name="season_focus">all_season</field>
        </record>

        <record id="toddler_sales_team" model="sale.team">
            <field name="name">Toddler Sales Team</field>
            <field name="code">TODDLER</field>
            <field name="description">Sales team for toddler clothing (2-4 years)</field>
            <field name="age_group_focus">2-4</field>
            <field name="gender_focus">all</field>
            <field name="season_focus">all_season</field>
        </record>

        <record id="kids_sales_team" model="sale.team">
            <field name="name">Kids Sales Team</field>
            <field name="code">KIDS</field>
            <field name="description">Sales team for kids clothing (4-12 years)</field>
            <field name="age_group_focus">4-12</field>
            <field name="gender_focus">all</field>
            <field name="season_focus">all_season</field>
        </record>

        <record id="teen_sales_team" model="sale.team">
            <field name="name">Teen Sales Team</field>
            <field name="code">TEEN</field>
            <field name="description">Sales team for teen clothing (12-16 years)</field>
            <field name="age_group_focus">12-16</field>
            <field name="gender_focus">all</field>
            <field name="season_focus">all_season</field>
        </record>

        <!-- Kids Clothing Specific Sales Territories -->
        <record id="boys_territory" model="sale.territory">
            <field name="name">Boys Territory</field>
            <field name="code">BOYS</field>
            <field name="description">Territory for boys clothing</field>
            <field name="age_group_focus">all</field>
            <field name="gender_focus">boys</field>
            <field name="season_focus">all_season</field>
        </record>

        <record id="girls_territory" model="sale.territory">
            <field name="name">Girls Territory</field>
            <field name="code">GIRLS</field>
            <field name="description">Territory for girls clothing</field>
            <field name="age_group_focus">all</field>
            <field name="gender_focus">girls</field>
            <field name="season_focus">all_season</field>
        </record>

        <record id="unisex_territory" model="sale.territory">
            <field name="name">Unisex Territory</field>
            <field name="code">UNISEX</field>
            <field name="description">Territory for unisex clothing</field>
            <field name="age_group_focus">all</field>
            <field name="gender_focus">unisex</field>
            <field name="season_focus">all_season</field>
        </record>

        <!-- Kids Clothing Specific Sales Commissions -->
        <record id="baby_commission" model="sale.commission">
            <field name="name">Baby Commission</field>
            <field name="code">BABY</field>
            <field name="description">Commission for baby clothing (0-2 years)</field>
            <field name="commission_type">percentage</field>
            <field name="commission_rate">7.0</field>
            <field name="age_group_focus">0-2</field>
            <field name="gender_focus">all</field>
            <field name="season_focus">all_season</field>
            <field name="state">active</field>
        </record>

        <record id="toddler_commission" model="sale.commission">
            <field name="name">Toddler Commission</field>
            <field name="code">TODDLER</field>
            <field name="description">Commission for toddler clothing (2-4 years)</field>
            <field name="commission_type">percentage</field>
            <field name="commission_rate">6.0</field>
            <field name="age_group_focus">2-4</field>
            <field name="gender_focus">all</field>
            <field name="season_focus">all_season</field>
            <field name="state">active</field>
        </record>

        <record id="kids_commission" model="sale.commission">
            <field name="name">Kids Commission</field>
            <field name="code">KIDS</field>
            <field name="description">Commission for kids clothing (4-12 years)</field>
            <field name="commission_type">percentage</field>
            <field name="commission_rate">5.0</field>
            <field name="age_group_focus">4-12</field>
            <field name="gender_focus">all</field>
            <field name="season_focus">all_season</field>
            <field name="state">active</field>
        </record>

        <record id="teen_commission" model="sale.commission">
            <field name="name">Teen Commission</field>
            <field name="code">TEEN</field>
            <field name="description">Commission for teen clothing (12-16 years)</field>
            <field name="commission_type">percentage</field>
            <field name="commission_rate">4.0</field>
            <field name="age_group_focus">12-16</field>
            <field name="gender_focus">all</field>
            <field name="season_focus">all_season</field>
            <field name="state">active</field>
        </record>
    </data>
</odoo>